					["custom"] = "aura_env.playerGUID = UnitGUID(\"player\")\naura_env.critDamageMultiplier = (select(5, GetTalentInfo(2, 23)) / 10 + 1) * (GetInventoryItemGems(1) == 41398 and 1.03 or 1)\n\naura_env.debuffs = {\n    [58683] = \"Savage Combat\",\n    [58684] = \"Savage Combat\",\n    [30069] = \"Blood Frenzy\",\n    [30070] = \"Blood Frenzy\",\n    [30708] = \"Totem of Wrath\",\n    --[48566] = \"Mangle (Cat)\",\n    --[33983] = \"Mangle (Cat)\",\n    --[48564] = \"Mangle (Bear)\",\n    --[46857] = \"Trauma\",\n    [49800] = \"Rip\",\n    [1079] = 1,\n    [9492] = 1,\n    [9493] = 1,\n    [9752] = 1,\n    [9894] = 1,\n    [9896] = 1,\n    [27008] = 1,\n    [49799] = 1,\n}\naura_env.ripIds = {\n    [1079] = 1,\n    [9492] = 1,\n    [9493] = 1,\n    [9752] = 1,\n    [9894] = 1,\n    [9896] = 1,\n    [27008] = 1,\n    [49799] = 1,\n    [49800] = 1\n}\n\n\nif WOW_PROJECT_ID and( WOW_PROJECT_ID == 11 or WOW_PROJECT_ID == WOW_PROJECT_BURNING_CRUSADE_CLASSIC) then\n    aura_env.argIndex = {\n        [\"srcGUID\"] = 2,\n        [\"dstGUID\"] = 6,\n        [\"spellID\"] = 10\n    }\nelse\n    aura_env.argIndex = {\n        [\"srcGUID\"] = 1,\n        [\"dstGUID\"] = 4,\n        [\"spellID\"] = 7\n    } \nend\n\naura_env.bleedDebuff = {\n    --[48566] = \"Mangle (Cat)\",\n    --[33983] = \"Mangle (Cat)\",\n    --[48564] = \"Mangle (Bear)\",\n    --[46857] = \"Trauma\"\n}\n\naura_env.damageMod = {\n    [58683] = \"Savage Combat\",\n    [58684] = \"Savage Combat\",\n    [30069] = \"Blood Frenzy\",\n    [30070] = \"Blood Frenzy\",\n}\n\nfunction aura_env.getNormalRipHit(mangleMultiplier, damageModMultiplier, total)\n    local base, posBuff, negBuff = UnitAttackPower(\"player\")\n    local currentAP = base + posBuff - negBuff\n    local damageMultiplier = select(7, UnitDamage(\"player\"))\n    local baseMultiplier = (currentAP * .05 + 501)\n    local varMultiplier = (mangleMultiplier or 1)*(damageModMultiplier or 1)--*(total and 11 or 1)\n    \n    return baseMultiplier * damageMultiplier * varMultiplier\nend",
