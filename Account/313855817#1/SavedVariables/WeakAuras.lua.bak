						["event"] = "Talent Known",
						["use_unit"] = true,
						["events"] = "CLEU:SPELL_AURA_APPLIED:SPELL_AURA_REFRESH:SPELL_AURA_REMOVED:SPELL_CAST_SUCCESS, PLAYER_REGEN_ENABLED, PLAYER_TARGET_CHANGED, PLAYER_DAMAGE_DONE_MODS, UNIT_ATTACK_POWER",
						["custom"] = "-- CLEU:SPELL_AURA_APPLIED:SPELL_AURA_REFRESH:SPELL_AURA_REMOVED:SPELL_CAST_SUCCESS, PLAYER_REGEN_ENABLED, PLAYER_TARGET_CHANGED, PLAYER_DAMAGE_DONE_MODS, UNIT_ATTACK_POWER\nfunction(allstates, event, unitId, subevent, ...)\n    if not aura_env.config[\"ripPowerOption\"] then return end\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        \n        local srcGUID, dstGUID, spellID = select(aura_env.argIndex[\"srcGUID\"], ...), select(aura_env.argIndex[\"dstGUID\"], ...), select(aura_env.argIndex[\"spellID\"], ...)\n        \n        if (subevent == \"SPELL_AURA_APPLIED\" or subevent == \"SPELL_AURA_REFRESH\" or subevent == \"SPELL_AURA_REMOVED\") then\n            if (aura_env.debuffs[spellID]) then\n                local state = allstates[dstGUID]\n                if not state then\n                    local mangleMultiplier = (aura_env.bleedDebuff[spellID] and subevent ~= \"SPELL_AURA_REMOVED\" and 1.3 or 1)\n                    local damageModMultiplier = (aura_env.damageMod[spellID] and subevent ~= \"SPELL_AURA_REMOVED\" and 1.04 or 1)\n                    allstates[dstGUID] = {\n                        show = true,\n                        changed = true,\n                        normalRip = aura_env.getNormalRipHit(mangleMultiplier, damageModMultiplier),\n                        mangleMultiplier = 1, --mangleMultiplier,\n                        damageModMultiplier = damageModMultiplier,\n                        visible = true\n                    }\n                    state = allstates[dstGUID]\n                elseif state then\n                    if (subevent == \"SPELL_AURA_REMOVED\") then\n                        state.mangleMultiplier = 1--(aura_env.bleedDebuff[spellID] and 1 or state.mangleMultiplier)\n                        state.damageModMultiplier = (aura_env.damageMod[spellID] and 1 or state.damageModMultiplier)\n                    else\n                        state.mangleMultiplier = 1--(aura_env.bleedDebuff[spellID] and 1.3 or state.mangleMultiplier)\n                        state.damageModMultiplier = (aura_env.damageMod[spellID] and 1.04 or state.damageModMultiplier)\n                    end\n                    \n                    state.normalRip = aura_env.getNormalRipHit(state.mangleMultiplier, state.damageModMultiplier)\n                    state.changed = true\n                    \n                end\n                if subevent == \"SPELL_AURA_REMOVED\" and aura_env.ripIds[spellID] and srcGUID == aura_env.playerGUID then\n                    state.appliedRip = nil\n                    state.percentage = 100;\n                    \n                    return true\n                end\n                state.changed = true\n                state.percentage = math.floor(state.normalRip/(state.appliedRip or state.normalRip)*100+0.5)\n                return true\n            end\n        elseif subevent == \"SPELL_CAST_SUCCESS\" and aura_env.ripIds[spellID] and srcGUID == aura_env.playerGUID then\n            local state = allstates[dstGUID]\n            if not state then\n                allstates[dstGUID] = {\n                    show = true,\n                    changed = true,\n                    normalRip = aura_env.getNormalRipHit(),\n                    visible = true\n                }\n                state = allstates[dstGUID]\n            elseif state then\n                state.normalRip = aura_env.getNormalRipHit(state.mangleMultiplier, state.damageModMultiplier)\n                state.changed = true\n            end\n            state.percentage = 100\n            state.appliedRip = state.normalRip\n        end\n    elseif event == \"PLAYER_TARGET_CHANGED\" then\n        local dstGUID = UnitGUID(\"target\")\n        for GUID, state in pairs(allstates) do\n            state.visible = dstGUID == GUID;\n            state.changed = true;\n        end\n        return true\n    elseif event == \"PLAYER_DAMAGE_DONE_MODS\" then\n        local dstGUID = UnitGUID(\"target\")\n        if (not dstGUID or not UnitCanAttack(\"player\", \"target\")) then\n            return false\n        end\n        \n        local state = allstates[dstGUID]\n        if not state then\n            allstates[dstGUID] = {\n                show = true,\n                changed = true,\n                normalRip = aura_env.getNormalRipHit(),\n                visible = true\n            }\n            state = allstates[dstGUID]\n        elseif state then\n            state.normalRip = aura_env.getNormalRipHit(state.mangleMultiplier, state.damageModMultiplier)\n            state.changed = true\n        end\n        state.percentage = math.floor(state.normalRip/(state.appliedRip or state.normalRip)*100+0.5)\n        return true\n    elseif event == \"UNIT_ATTACK_POWER\" then\n        if (unitId ~= \"player\") then\n            return false\n        end\n        \n        local dstGUID = UnitGUID(\"target\")\n        if (not dstGUID or not UnitCanAttack(\"player\", \"target\")) then\n            return false\n        end\n        local state = allstates[dstGUID]\n        if not state then\n            allstates[dstGUID] = {\n                show = true,\n                changed = true,\n                normalRip = aura_env.getNormalRipHit(),\n                visible = true\n            }\n            state = allstates[dstGUID]\n        elseif state then\n            state.normalRip = aura_env.getNormalRipHit(state.mangleMultiplier, state.damageModMultiplier)\n            state.changed = true\n        end\n        state.percentage = math.floor(state.normalRip/(state.appliedRip or state.normalRip)*100+0.5)\n        return true\n    end\nend",
